<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>secj</servlet-name>
        <servlet-class>secj</servlet-class>
    </servlet>
    <context-param>
        <param-name>username</param-name>
        <param-value>jyothi</param-value>
    </context-param>
    <context-param>
        <param-name>password</param-name>
        <param-value>jyothi</param-value>
    </context-param>
    <servlet>
        <servlet-name>Adminj</servlet-name>
        <servlet-class>Adminj</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Userj</servlet-name>
        <servlet-class>Userj</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Mongodb</servlet-name>
        <servlet-class>Mongodb</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pdf</servlet-name>
        <servlet-class>pdf</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>NewServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>secj</servlet-name>
        <url-pattern>/secj</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Adminj</servlet-name>
        <url-pattern>/Adminj</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Userj</servlet-name>
        <url-pattern>/Userj</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Mongodb</servlet-name>
        <url-pattern>/Mongodb</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pdf</servlet-name>
        <url-pattern>/pdf</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
  
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
